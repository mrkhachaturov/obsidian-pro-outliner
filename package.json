{
  "name": "obsidian-pro-outliner",
  "version": "1.0.0",
  "description": "Work with your lists like in Workflowy or RoamResearch, with zoom functionality.",
  "main": "main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.mjs -w --configWithTests",
    "build-with-tests": "rollup --config rollup.config.mjs --configWithTests",
    "build": "rollup --config rollup.config.mjs",
    "lint": "prettier --check src && eslint src",
    "format": "prettier --write src",
    "test": "jest --forceExit",
    "test:unit": "SKIP_OBSIDIAN=1 jest --forceExit src",
    "prepare": "husky"
  },
  "author": "Ruben Khachaturov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@codemirror/language": "^6.10.8",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.36.3",
    "@eslint/js": "^9.21.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^12.1.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/diff": "^7.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.8",
    "babel-jest": "^29.7.0",
    "classic-level": "^2.0.0",
    "debug": "^4.4.0",
    "eslint": "^9.21.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "obsidian": "latest",
    "prettier": "^3.5.2",
    "rollup": "^4.34.9",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "ws": "^8.18.1"
  }
}
